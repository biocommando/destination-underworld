# on kill_count: 12 do spawn: 10, 5, 0, 0, 100, 0, 0
# on kill_count: 12 do spawn: 10, 6, 0, 0, 100, 0, 0
# 
# 
# // Reveal exit
# on kill_count: 24 do modify_terrain: x = 7, y = 2, terrain_type = floor
#end_script
