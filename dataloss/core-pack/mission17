# on positional_trigger: 0 do spawn: 11, 1, 0, 0, 0, 0, 100
# on positional_trigger: 0 do spawn: 11, 2, 0, 0, 0, 0, 100
# on positional_trigger: 0 do spawn: 11, 3, 0, 0, 0, 0, 100
# on positional_trigger: 0 do spawn: 11, 4, 0, 0, 0, 0, 100
# on positional_trigger: 0 do spawn: 10, 5, 0, 0, 0, 0, 100
# on positional_trigger: 0 do spawn: 10, 6, 0, 0, 0, 0, 100
# 
# // spawn exit... (not accessible yet)
# on kill_count: 12 do modify_terrain: x = 7, y = 1, terrain_type = level_exit
# 
# on kill_count: 12 do modify_terrain: x = 6, y = 7, terrain_type = floor
# on kill_count: 12 do modify_terrain: x = 8, y = 7, terrain_type = floor
# 
# on kill_count: 12 do modify_terrain: x = 6, y = 8, terrain_type = floor
# on kill_count: 12 do modify_terrain: x = 8, y = 8, terrain_type = floor
# on kill_count: 12 do modify_terrain: x = 9, y = 8, terrain_type = floor
# 
# on [explosion_madness_handicap1] @never do @nothing
# on [explosion_madness_handicap2] @never do @nothing
# on [explosion_madness_handicap3] @never do @nothing
# on [explosion_madness_handicap4] @never do @nothing
# on {# override_event('explosion_madness_handicap1', 'explosion_madness') #} kill_count: 6 do modify_terrain: x = 7, y = 9, terrain_type = floor
# on {# override_event('explosion_madness_handicap2', 'explosion_madness') #} kill_count: 7 do modify_terrain: x = 7, y = 10, terrain_type = floor
# on {# override_event('explosion_madness_handicap3', 'explosion_madness') #} kill_count: 8 do modify_terrain: x = 8, y = 10, terrain_type = floor
# on {# override_event('explosion_madness_handicap4', 'explosion_madness') #} kill_count: 9 do modify_terrain: x = 9, y = 10, terrain_type = floor
# 
# // then spawn imps
# on kill_count: 12 do spawn: 4, 1, 0, 0, 100, 0, 0
# on kill_count: 12 do spawn: 4, 2, 0, 0, 100, 0, 0
# on kill_count: 12 do spawn: 4, 3, 0, 0, 100, 0, 0
# on kill_count: 12 do spawn: 4, 4, 0, 0, 100, 0, 0
