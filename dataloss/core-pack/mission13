# ignore_init_properties
# on positional_trigger: 0 do modify_terrain: x = 7, y = 5, terrain_type = floor
# on positional_trigger: 0 do modify_terrain: x = 9, y = 5, terrain_type = floor
#end_script
#script 2
# ignore_init_properties
# on kill_count: 8 do modify_terrain: x = 12, y = 7, terrain_type = floor
# on kill_count: 15 do modify_terrain: x = 13, y = 10, terrain_type = floor
# on positional_trigger: 0 do modify_terrain: x = 14, y = 3, terrain_type = floor
#end_script
